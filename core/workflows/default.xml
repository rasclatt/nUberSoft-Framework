			<function>
				<name>autoload_core_functions</name>
				<include>/core/functions/function.autoload_core_functions.php</include>
				<inject>nuber</inject>
			</function>
		</object>
		<object>
			<class>
				<name>\Nubersoft\nReWriter</name>
				<method>validate</method>
			</class>
		</object>
		<object>
			<class>
				<name>\Nubersoft\nObserverProcessor</name>
				<method>listen</method>
			</class>
		</object>
		<object>
			<class>
				<name>\Nubersoft\nCache</name>
				<method>startCache</method>
			</class>
