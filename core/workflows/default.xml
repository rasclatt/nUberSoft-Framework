			<class>
				<name>\Nubersoft\nObserverTemplate</name>
				<method>listen</method>
			</class>
		</object>
		<object>
			<function>
				<name>get_timestamp</name>
				<include>/core/functions/function.get_timestamp.php</include>
			</function>
		</object>
		<object>
			<function>
				<name>AutoForward</name>
				<include>/core/functions/function.AutoForward.php</include>
			</function>
		</object>
		<object>
			<class>
				<name>\Nubersoft\nAutomator</name>
				<method>saveCachedConfigs</method>
			</class>
		</object>
	</workflow>
