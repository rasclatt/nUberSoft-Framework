<?xml version="1.0" encoding="UTF-8"?>
<config>

	<action event="nbr_cart_clear" after="nbr_session_to_node" request="get">
		<object event="nbr_cart_clear">
			<class>
				<name>\nPlugins\Nubersoft\ShoppingCart\Model</name>
				<method>clear</method>
			</class>
		</object>
	</action>
	
	<action event="nbr_cart_load_regions" request="post" is_ajax="true">
		<object event="nbr_cart_load_regions">
			<class>
				<name>\nPlugins\Nubersoft\ShoppingCart\View</name>
				<method>placeRegion</method>
			</class>
		</object>
	</action>
	
	<action event="nbr_check_user_credentials" request="post" is_ajax="true">
		<object event="nbr_check_user_credentials">
			<class>
				<name>\nPlugins\Nubersoft\CoreLogin</name>
				<method>isSiteUser</method>
			</class>
		</object>
	</action>
	
	<action event="nbr_login_create" request="post" is_ajax="true">
		<object event="nbr_login_create">
			<class>
				<name>\nPlugins\Nubersoft\CoreLogin</name>
				<method>convertFormDataToPost</method>
				<chain>loginByRequest</chain>
			</class>
		</object>
	</action>

	<action event="nbr_login_with_code" request="post">
		<object event="nbr_login_with_code">
			<class>
				<name>\nPlugins\Nubersoft\CoreLogin</name>
				<method>loginWithCode</method>
			</class>
		</object>
	</action>
	<action event="nbr_check_admin_code" request="post" is_ajax="true">
		<object event="nbr_check_admin_code">
			<class>
				<name>\Nubersoft\nCommunications</name>
				<method>observer</method>
			</class>
		</object>
	</action>
	<action event="create_curl_cache" request="get">
		<object event="create_curl_cache">
			<class>
				<name>\nPlugins\Nubersoft\Cache</name>
				<method>buildStaticCache</method>
			</class>
		</object>
	</action>
	<action event="nbr_make_path_viewable" after="nbr_set_registry" request="post">
		<object event="nbr_make_path_viewable">
			<class>
				<name>\nPlugins\Nubersoft\CorePreferences</name>
				<method>makeDirReadable</method>
			</class>
		</object>
	</action>
	<action event="nbr_reset_table_data" after="nbr_set_registry" request="request">
		<object event="nbr_reset_table_data">
			<class>
				<name>\Nubersoft\CoreMySQL</name>
				<method>resetTable</method>
			</class>
		</object>
	</action>
	<action event="nbr_save_edits" after="nbr_set_registry" request="post">
		<object event="nbr_save_edits">
			<class>
				<name>\nPlugins\Nubersoft\CoreDatabase</name>
				<method>saveComponent</method>
			</class>
		</object>
	</action>
	<action event="nbr_insert_dup_component" after="nbr_set_registry" request="post">
		<object event="nbr_insert_dup_component">
			<class>
				<name>\nPlugins\Nubersoft\CoreDatabase</name>
				<method>saveDuplicateComponent</method>
			</class>
		</object>
	</action>
	<action event="nbr_change_component_name" after="nbr_set_registry" request="post" is_ajax="true">
		<object event="nbr_change_component_name">
			<class>
				<name>\nPlugins\Nubersoft\CoreDatabase</name>
				<method>renameComponent</method>
			</class>
		</object>
	</action>
	<action event="nbr_get_form_token" after="nbr_set_registry" request="post">
		<object event="nbr_get_form_token">
			<class>
				<name>\Nubersoft\GetSitePrefs</name>
				<method>getSetToken</method>
				<dependency>\Nubersoft\nToken</dependency>
			</class>
		</object>
	</action>
	<action event="nbr_delete_cache_folder" after="nbr_set_pageuri" request="post" is_admin="true">
		<object event="nbr_delete_cache_folder">
			<class>
				<name>\Nubersoft\HeadProcessor</name>
				<method>deleteCacheFolder</method>
			</class>
		</object>
	</action>
	<action event="nbr_delete_cache_folder_ajax" after="nbr_general_redirect" request="post" is_admin="true">
		<object event="nbr_delete_cache_folder_ajax">
			<class>
				<name>\Nubersoft\HeadProcessor</name>
				<method>deleteCacheFolder</method>
				<inject>
					<arg>/cache_dir.pref</arg>
					<arg>/template.pref</arg>
				</inject>
			</class>
			<class>
				<name>\Nubersoft\HeadProcessor</name>
				<method>reportAjax</method>
			</class>
		</object>
	</action>
	<action event="nbr_admin_toggle" after="nbr_system_defines" request="post">
		<object event="nbr_admin_toggle">
			<class>
				<name>\nPlugins\Nubersoft\InspectorPallet</name>
				<method>validateToggle</method>
			</class>
		</object>
	</action>
	<action event="nbr_duplicate_component" after="nbr_set_registry" request="post" is_admin="true">
		<object event="nbr_duplicate_component">
			<class>
				<name>\nPlugins\Nubersoft\CoreDatabase</name>
				<method>duplicateComponent</method>
			</class>
		</object>
	</action>
	<action event="nbr_delete_component" after="nbr_set_registry" request="post" is_admin="true">
		<object event="nbr_delete_component">
			<class>
				<name>\nPlugins\Nubersoft\CoreDatabase</name>
				<method>deleteComponent</method>
			</class>
		</object>
	</action>
	<action event="nbr_delete_component_filename" after="nbr_set_registry" request="post" is_admin="true">
		<object event="nbr_delete_component_filename">
			<class>
				<name>\nPlugins\Nubersoft\CoreDatabase</name>
				<method>deleteComponentImage</method>
			</class>
		</object>
	</action>
	<action event="nbr_edit_component_filename" after="nbr_set_registry" request="post" is_admin="true">
		<object event="nbr_edit_component_filename">
			<class>
				<name>\nPlugins\Nubersoft\CoreDatabase</name>
				<method>updateComponentImage</method>
			</class>
		</object>
	</action>
	<action event="nbr_save_menu" after="nbr_set_registry" request="post" is_admin="true">
		<object event="nbr_save_menu">
			<class>
				<name>\nPlugins\Nubersoft\CoreRouter</name>
				<method>updateMenu</method>
			</class>
		</object>
	</action>
	<action event="nbr_delete_menu" after="nbr_set_registry" request="post" is_admin="true">
		<object event="nbr_delete_menu">
			<class>
				<name>\nPlugins\Nubersoft\CoreRouter</name>
				<method>deleteMenu</method>
			</class>
		</object>
	</action>
	<action event="nbr_save_interface_pref" after="nbr_set_registry" request="post" is_admin="true">
		<object event="nbr_save_interface_pref">
			<class>
				<name>\nPlugins\Nubersoft\CorePreferences</name>
				<method>saveAdminUserPref</method>
			</class>
		</object>
	</action>
	<action event="nbr_get_interface_pref" after="nbr_set_registry" request="post" is_admin="true">
		<object event="nbr_get_interface_pref">
			<class>
				<name>\nPlugins\Nubersoft\CorePreferences</name>
				<method>getAdminUserPref</method>
			</class>
		</object>
	</action>
	<action event="nbr_edit_table_row" after="nbr_set_registry" request="post" is_admin="true">
		<object event="nbr_edit_table_row">
			<class>
				<name>\nPlugins\Nubersoft\CoreTables</name>
				<method>saveRequestData</method>
			</class>
		</object>
	</action>
	<action event="nbr_run_raw_query" after="nbr_set_registry" request="post" is_admin="true">
		<object event="nbr_run_raw_query">
			<class>
				<name>\nPlugins\Nubersoft\CoreDatabase</name>
				<method>runRawQuery</method>
			</class>
		</object>
	</action>
	<action event="nbr_create_menu" after="nbr_set_registry" request="post">
		<object event="nbr_create_menu">
			<class>
				<name>\nPlugins\Nubersoft\CoreRouter</name>
				<method>saveComponent</method>
			</class>
		</object>
	</action>
	<action event="nbr_menu_button_save" after="nbr_set_registry" request="post">
		<object event="nbr_menu_button_save">
			<class>
				<name>\nPlugins\Nubersoft\CoreMenu</name>
				<method>useTable</method>
				<inject>menu_display</inject>
				<chain>saveComponent</chain>
			</class>
		</object>
	</action>
	<action event="nbr_download_file" after="nbr_set_registry" request="request">
		<object event="nbr_download_file">
			<class dependency="\Nubersoft\nDownloader">
				<name>\nPlugins\Nubersoft\CoreDownloader</name>
				<method>startDownload</method>
				<chain>download</chain>
				<dependency>
					<inject>\Nubersoft\nDownloader</inject>
					<into>startDownload</into>
				</dependency>
			</class>
		</object>
	</action>
	<action event="download" after="nbr_set_registry" request="request">
		<object event="download">
			<class>
				<name>\nPlugins\Nubersoft\CoreDownloader</name>
				<method>directDownload</method>
				<dependency>\Nubersoft\nDownloader</dependency>
			</class>
		</object>
	</action>
	<action event="ajax_edit_logo" after="nbr_set_registry" request="post">
		<object event="ajax_edit_logo">
			<class>
				<name>\nPlugins\Nubersoft\CoreFileHandler</name>
				<method>formDataManager</method>
				<dependency>\Nubersoft\nApp</dependency>
			</class>
		</object>
	</action>
	<action event="nbr_deploy_changes">
		<object event="nbr_deploy_changes" after="nbr_set_registry" request="get" is_admin="true">
			<class>
				<name>\nPlugins\Nubersoft\CoreInstaller</name>
				<method>deployPackage</method>
				<dependency>\Nubersoft\nApp</dependency>
			</class>
		</object>
	</action>
</config>