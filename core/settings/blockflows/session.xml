<?xml version="1.0" encoding="UTF-8"?>
<config>
	<workflow>
		<object event="nbr_nsessioner_construct">
			<class>
				<name>\Nubersoft\nSessioner</name>
				<inject>s</inject>
			</class>
		</object>
		<object event="nbr_nsessioner_observe">
			<class>
				<name>\Nubersoft\nSessioner</name>
				<method>observer</method>
			</class>
		</object>
		<object event="nbr_session_to_node">
			<class>
				<name>\Nubersoft\Submits</name>
				<method>setSessionGlobal</method>
			</class>
		</object>
	</workflow>
</config>