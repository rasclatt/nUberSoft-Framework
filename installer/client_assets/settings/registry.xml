<?xml version="1.0" encoding="UTF-8"?>
<register>
	<!--ADDS .htaccess to close off file permission to browser including child folders
	***WARNING:***
	It is not advisable to remove security on these folders
	CONFIG FILES MAY BE EXPOSED!
	-->
	<protectdirectory>
		<var>/core.processor/renderlib/</var>
		<var>/core.processor/engine/</var>
		<var>/core.processor/classes/</var>
		<var>/core.processor/functions/</var>
		<var>/core.plugins/plugin.SaveHtaccess/</var>
		<var>/core.plugins/plugin.RefreshDefines/</var>
		<var>/core.ajax/functions/</var>
		<var>/core.ajax/confirm/</var>
		<var>/client_assets/settings/</var>
	</protectdirectory>
	<!--REMOVES .htaccess files to allow direct access via browser -->
	<unprotectdirectory>
		<var>/core.processor/</var>
	</unprotectdirectory>
	<!--ADDS .htaccess files to allow direct access via browser. Made to override parent folder no-access permissions -->
	<unprotectset>
		<var>/core.plugins/plugin.SaveHtaccess/images/</var>
		<var>/core.plugins/plugin.RefreshDefines/images/</var>
		<var>/core.processor/renderlib/admintools/images/</var>
	</unprotectset>
	<onload>
		<classes>
			<coreLoader>
				<dir>/client_assets/plugins/coreLoader/</dir>
				<instr>replace/core</instr>
				<method>getCore</method>
			</coreLoader>
		</classes>
	</onload>
	<onlogin>
		<classes>
		</classes>
	</onlogin>
	<admintoolsmenu>
		<menu>
			<name>PAGE EDITOR</name>
			<url>~app::site_url~~app::fetch_admin_link~?page_editor=true</url>
		</menu>
		<menu>
			<name>HOME PAGE</name>
			<url>~app::site_url~/</url>
		</menu>
	</admintoolsmenu>
	<ondefine>
		<dbengine>MYSQL</dbengine>
		<webmaster>no-reply@iwantcreative.com</webmaster>
		<server_mode>true</server_mode>
		<session_on>true</session_on>
		<allow_eval>false</allow_eval>
		<f_access>true</f_access>
		<file_salt>saltypQleSI121</file_salt>
		<max_up_files>2000</max_up_files>
		<base_url>http://www.nubersoft.com</base_url>
		<base_url_ssl>https://www.nubersoft.com</base_url_ssl>
		<force_url_ssl>false</force_url_ssl>
		<session_expire_time>3500</session_expire_time>
		<temp_dir>/../tempfiles/</temp_dir>
		<cache_dir>/../cachefiles/</cache_dir>
		<sys_prefs_temp>false</sys_prefs_temp>
		<openssl_salt>0294019347292093</openssl_salt>
		<openssl_iv>6392847651093762</openssl_iv>
		<nbr_superuser>1</nbr_superuser>
		<nbr_admin>2</nbr_admin>
		<nbr_web>3</nbr_web>
	</ondefine>
	<onloadconfigfunctions>
		<nloader />
		<check_empty />
		<error_check />
		<fetch_table_id />
		<fetch_table_name />
		<fetch_token />
		<format_input />
		<get_timezone />
		<get_clientfunctions />
		<get_file_extension />
		<is_defined />
		<is_admin />
		<is_loggedin />
		<load_clientfunctions />
		<load_client_config />
		<nuber_faux />
		<nQuery />
		<organize />
		<printpre />
		<register_use />
		<silent_error />
		<site_live />
		<site_url />
		<site_valid />
		<table_valid />
	</onloadconfigfunctions>
	<onloadnuberfunctions>
		<apply_markup />
		<AutoForward />
		<ChangePassword />
		<decode_serial />
		<get_bypass />
		<get_form_layout />
		<get_header />
		<get_header_options />
		<get_menubar />
		<get_now_status />
		<get_site_options />
		<get_site_prefs />
		<get_template />
		<get_timestamp />
		<initialize_edit_mode />
		<login_window />
		<process_requests />
		<render_admintools />
		<render_content />
		<render_contentcached />
		<render_error />
		<render_footer />
		<render_header />
		<render_masthead />
		<validate_page />
	</onloadnuberfunctions>
	<admintoolstables>
		<components>
			<col>ref_page</col>
			<col>ref_spot</col>
			<col>parent_id</col>
			<col>ref_anchor</col>
			<col>ref_spot</col>
			<col>component_type</col>
			<col>content</col>
			<col>page_live</col>
		</components>
		<component_builder>
			<col>component_name</col>
			<col>assoc_table</col>
			<col>component_value</col>
			<col>variable_type</col>
			<col>page_order</col>
			<col>page_live</col>
		</component_builder>
		<dropdown_menus>
			<col>assoc_column</col>
			<col>menuName</col>
			<col>menuVal</col>
			<col>page_order</col>
			<col>page_live</col>
		</dropdown_menus>
		<file_types>
			<col>file_extension</col>
			<col>file_type</col>
			<col>page_order</col>
			<col>page_live</col>
		</file_types>
		<form_builder>
			<col>column_type</col>
			<col>column_name</col>
			<col>size</col>
			<col>page_order</col>
			<col>page_live</col>
		</form_builder>
		<members_connected>
			<col>ip_address</col>
			<col>username</col>
			<col>timestamp</col>
		</members_connected>
		<system_settings>
			<col>page_live</col>
			<col>page_element</col>
		</system_settings>
	</admintoolstables>
</register>